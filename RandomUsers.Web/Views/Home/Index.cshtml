@model IEnumerable<RandomUsers.Web.Models.UserModel>

<h2>Users List</h2>

<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>           
            <th>Gender</th>
            <th>Cell</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Name.First</td>
                <td>@user.Name.Last</td>
                <td>@user.Email</td>               
                <td>@user.Gender</td>
                <td>@user.Cell</td>
            </tr>
        }
    </tbody>
</table>
